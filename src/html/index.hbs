<!doctype html>
<html lang='en' data-version='{{timestamp}}'>

<head>
	{{> base/extraScripts}}

	{{> base/meta}}
	
	{{> base/stylesheet}}
	
	{{> base/font}}

	{{> base/analytics }}
</head>

<body>
	{{> base/header 'sticky'}}

	<main id='content'>

		<div class='intro'>
			<h1 class='intro__hed'>Title Wanted</h1>
			<p class='intro__dek'>A subtitle goes here.</p>
			<p class='intro__byline'>By
				<a href='http://dnewcomb.com' class="link-text" target='_blank'>
					<strong>David Newcomb</strong>
				</a>
			</p>
			<time class='intro__time'>July 2020</time>
			<div class='intro__prelude'>
				<p></p>
			</div>
			</div>
		</div>

		{{> base/localScripts}}
	</main>

	{{> base/footer }}

	{{> base/graphic}}
</body>

</html>
